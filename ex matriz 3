#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {
	
	int tab1 [4] [4];
	int tabResult [2] [2];
	int i,j; //i = linha, j = coluna
	
	printf("Soma das Linhas e das Colunas\n");
	
	for(i = 0; i < 4; i++){
		for(j = 0; j < 4; j++){
			printf("Digite o numero da linha %d e da coluna %d da tabela 1: ", i + 1, j + 1);
			scanf("%d", &tab1[i][j]);
		}
	}
	printf("\nTabela 1\n");
	for(i = 0; i < 4; i++){
		for(j = 0; j < 4; j++){
			printf("|");
			printf(" %d |", tab1[i][j]);
		}
		printf("\n");
	}
	

		
	return 0;
}
