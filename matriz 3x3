#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {
	
	int tab [3] [3];
	int i,j; //i = linha, j = coluna
	
	printf("Crie uma tabela\n");
	
	for(i = 0; i < 3; i++){
		for(j = 0; j < 3; j++){
			printf("Digite o numero da linha %d e da coluna %d: ", i + 1, j + 1);
			scanf("%d", &tab[i][j]);
		}
	}
	
	printf("A Tabela criada e:\n");
	for(i = 0; i < 3; i++){
		for(j = 0; j < 3; j++){
			printf("|");
			printf(" %d |", tab[i][j]);
}
	printf("\n");
}
		
	return 0;
}
