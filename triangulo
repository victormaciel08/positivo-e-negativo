#include <stdio.h>

int main(void) {

	float l1 = 0;
	float l2 = 0;
	float l3 = 0;
	
	printf("\nDescubra se o triangulo e verdadeiro");
	printf("\nDigite o tamanho do primeiro lado: ");
	scanf("%f", &l1);
	printf("\nDigite o tamanho do segundo lado: ");
	scanf("%f", &l2);
	printf("\nDigite o tamanho do terceiro lado: ");
	scanf("%f", &l3);
	
	if (l1 + l2 < l3 || l2 + l3 < l1 || l1 + l3 < l2){
		printf("O triangulo nao existe.");
	}
	else {
		printf("Esse triangulo exite");
	}
	
	
	return 0;	
}
